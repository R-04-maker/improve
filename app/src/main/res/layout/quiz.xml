<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/judulquiz"
    android:layout_width="match_parent"
    android:layout_height="match_parent">


    <ImageView
        android:id="@+id/imageView10"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginBottom="220dp"
        android:src="@drawable/newiconkuis"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView11"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginTop="400dp"
        android:layout_marginBottom="-40dp"
        android:src="@drawable/btn_birutua"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <LinearLayout
        android:id="@+id/btnPengajuanRuangan"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:orientation="vertical"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/imageView10">


    </LinearLayout>

    <TextView
        android:id="@+id/tingkatkan3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:autoSizeMaxTextSize="35dp"
        android:autoSizeMinTextSize="10dp"
        android:autoSizeTextType="uniform"
        android:fontFamily="@font/poppins_bold"
        android:gravity="center"
        android:text="Tes Minat Anda"
        android:textAllCaps="false"
        android:textColor="@color/biru_tua"
        android:textSize="35dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/layoutMenuAtas" />

    <TextView
        android:id="@+id/tingkatkan5"
        android:layout_width="wrap_content"
        android:layout_height="100dp"
        android:autoSizeMaxTextSize="18dp"
        android:autoSizeMinTextSize="10dp"
        android:layout_marginTop="-20dp"
        android:autoSizeTextType="uniform"
        android:fontFamily="@font/poppins_bold"
        android:gravity="center"
        android:text=" Kuis ini dibuat untuk mengetahui \n minat anda,  lanjutkan ? " android:textAllCaps="false"
        android:textColor="@color/black"
        android:textSize="18dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tingkatkan3" />


    <LinearLayout
        android:id="@+id/layoutMenuAtas1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="30dp"
        android:elevation="0dp"
        android:orientation="horizontal"
        app:layout_constraintBottom_toBottomOf="parent">


        <LinearLayout
            android:id="@+id/btnPengajuanAsset1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="15dp"
            android:layout_weight="1"
            android:orientation="vertical">

            <TextView
                android:id="@+id/save"
                android:layout_width="match_parent"
                android:layout_height="40dp"
                android:layout_marginStart="0dp"
                android:layout_marginTop="10dp"
                android:layout_marginBottom="10dp"
                android:autoSizeMaxTextSize="20dp"
                android:autoSizeMinTextSize="10dp"
                android:autoSizeTextType="uniform"
                android:background="@drawable/btn_orange"
                android:enabled="false"
                android:fontFamily="@font/poppins_bold"
                android:gravity="center"
                android:text="Tidak"
                android:textAllCaps="false"
                android:textColor="@color/white"
                android:textSize="20dp" />


        </LinearLayout>
        <LinearLayout
            android:id="@+id/btnPengajuanAsset2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="15dp"
            android:layout_weight="1"
            android:orientation="vertical">

            <TextView
                android:id="@+id/saveya"
                android:layout_width="match_parent"
                android:layout_height="40dp"
                android:layout_marginStart="0dp"
                android:layout_marginTop="10dp"
                android:layout_marginBottom="10dp"
                android:autoSizeMaxTextSize="20dp"
                android:autoSizeMinTextSize="10dp"
                android:autoSizeTextType="uniform"
                android:background="@drawable/btn_orange"
                android:enabled="false"
                android:fontFamily="@font/poppins_bold"
                android:gravity="center"
                android:text="Ya"
                android:textAllCaps="false"
                android:textColor="@color/white"
                android:textSize="20dp" />


        </LinearLayout>
    </LinearLayout>
    <LinearLayout
        android:id="@+id/layoutMenuAtas"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:elevation="4dp"
        android:layout_marginTop="330dp"
        android:orientation="horizontal"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/btnPengajuanRuangan">

        <LinearLayout
            android:id="@+id/btnPengajuanRuangan2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:orientation="vertical">

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <ImageView
                    android:id="@+id/btn_PengajuanRuangan"
                    android:layout_width="0dp"
                    android:layout_marginRight="0dp"
                    android:layout_height="120dp" />


            </RelativeLayout>

        </LinearLayout>

        <LinearLayout
            android:id="@+id/btnPengajuanAsset"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:orientation="vertical">

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <ImageView
                    android:id="@+id/btn_PengajuanAset"
                    android:layout_width="0dp"
                    android:layout_height="00dp"
                    android:layout_marginLeft="90dp"
                    android:scaleX="-1" />

            </RelativeLayout>

        </LinearLayout>

    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>